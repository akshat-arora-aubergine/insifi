---
import "../styles/global.css";
---

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />

<!-- Basic Meta Tags -->
<title>Insifi by Aubergine</title>
<meta
  name="description"
  content="Insifi by Aubergine"
/>
<meta name="keywords" content="keyword1, keyword2, keyword3" />
<meta name="author" content="Your Name or Company Name" />

<!-- Favicons -->
<link rel="icon" href="/favicon.png" sizes="any" />

<!-- Favicon for IE -->
<link rel="shortcut icon" href="/favicon.png" type="image/x-icon" />

<!-- Open Graph / Facebook -->
<meta property="og:image" content="/thumbnail.png" />
<meta property="og:type" content="website" />
<meta property="og:url" content="https://www.insifi.ai/" />
<meta property="og:title" content="Your Website Title" />
<meta
  property="og:description"
  content="Insifi by Aubergine"
/>

<!-- Twitter -->
<meta property="twitter:card" content="/thumbnail.png" />
<meta property="twitter:url" content="https://www.insifi.ai/" />
<meta property="twitter:title" content="Insifi by Aubergine" />
<meta
  property="twitter:description"
  content="Insifi by Aubergine"
/>
<meta
  property="twitter:image"
  content="/thumbnail.png"
/>

<!-- Canonical URL -->
<link rel="canonical" href="https://www.insifi.ai/" />

<!-- Apple Touch Icon (already included in favicons, but keeping for backwards compatibility) -->
<link rel="apple-touch-icon" sizes="180x180" href="/favicon.png" />

<!-- Theme Color for Mobile Browsers -->
<meta name="theme-color" content="#ffffff" />

<!---- Alpine  -->
<script is:inline defer src="https://unpkg.com/alpinejs@3.10.5/dist/cdn.min.js">
</script>
